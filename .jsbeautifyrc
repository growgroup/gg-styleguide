{
  "html": {
    "indent_size": 4,
    "indent_char": "\\t",
    "max_preserve_newlines": -1,
    "preserve_newlines": false,
    "keep_array_indentation": false,
    "break_chained_methods": false,
    "indent_scripts": "keep",
    "brace_style": "collapse",
    "space_before_conditional": false,
    "unescape_strings": false,
    "jslint_happy": false,
    "end_with_newline": false,
    "wrap_line_length": 0,
    "indent_inner_html": false,
    "comma_first": false,
    "e4x": false,
    "indent_empty_lines": false,
    "inline": [
      "abbr",
      "area",
      "audio",
      "bdi",
      "bdo",
      "br",
      "cite",
      "code",
      "data",
      "del",
      "dfn",
      "em",
      "embed",
      "i",
      "ins",
      "kbd",
      "keygen",
      "map",
      "mark",
      "math",
      "meter",
      "noscript",
      "object",
      "output",
      "progress",
      "q",
      "ruby",
      "s",
      "samp",
      "select",
      "small",
      "strong",
      "sub",
      "sup",
      "template",
      "time",
      "u",
      "var",
      "wbr",
      "text",
      "acronym",
      "big",
      "strike",
      "tt"
    ]
  }
}
